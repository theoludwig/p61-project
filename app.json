{
  "expo": {
    "name": "p61-project",
    "slug": "p61-project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./presentation/assets/images/icon.png",
    "scheme": "p61-project",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./presentation/assets/images/splashscreen.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#74b6cb"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./presentation/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./presentation/assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
